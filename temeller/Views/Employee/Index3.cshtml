@model List<Employee>
<div>
    <ul>
        @foreach(Employee item in Model){
            <div class="lead d-md-flex p-10 m-10">
                <h6>@item.FullName</h6>
                <p>@item.Id</p>
                <p>@item.Age</p>
                <p>@item.FirstName</p>
                <p>@item.LastName</p>
            </div>
        }
    </ul>
</div>